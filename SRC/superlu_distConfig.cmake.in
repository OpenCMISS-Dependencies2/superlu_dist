@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

set(enable_openmp @enable_openmp@)
set(TPL_ENABLE_INTERNAL_BLASLIB @TPL_ENABLE_INTERNAL_BLASLIB@)
set(TPL_ENABLE_LAPACKLIB @TPL_ENABLE_LAPACKLIB@)
set(TPL_ENABLE_CUDALIB @TPL_ENABLE_CUDALIB@)
set(TPL_ENABLE_HIPLIB @TPL_ENABLE_HIPLIB@)
set(TPL_ENABLE_PARMETIS @TPL_ENABLE_PARMETIS@)
set(TPL_ENABLE_COLAMD @TPL_ENABLE_COLAMD@)

list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

find_dependency(MPI REQUIRED)
if(TPL_ENABLE_CUDALIB)
  find_dependency(CUDA REQUIRED)
  find_dependency(CUDAToolkit REQUIRED)
endif()
if(enable_openmp)
  find_dependency(OpenMP)
endif()
if(TPL_ENABLE_HIPLIB)
  find_dependency(HIP MODULE)
endif()
if(NOT TPL_ENABLE_INTERNAL_BLASLIB)
  find_dependency(BLAS)
endif()
if(TPL_ENABLE_LAPACKLIB)
  find_dependency(LAPACK)
endif()
if(TPL_ENABLE_PARMETIS)
  find_dependency(ParMETIS)
endif()
if(TPL_ENABLE_COLAMD)
  find_dependency(COLAMD)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/superlu_distTargets.cmake")
